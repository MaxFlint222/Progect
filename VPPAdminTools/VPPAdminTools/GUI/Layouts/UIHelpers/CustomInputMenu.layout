FrameWidgetClass rootFrame {
 size 0.6 0.8
 halign center_ref
 valign center_ref
 hexactpos 0
 vexactpos 0
 hexactsize 0
 vexactsize 0
 {
  PanelWidgetClass FrameWidget {
   ignorepointer 1
   position 0 0
   size 1 1
   halign center_ref
   valign center_ref
   hexactpos 0
   vexactpos 0
   hexactsize 0
   vexactsize 0
   style editor_quad
   {
    PanelWidgetClass PnlBg {
     ignorepointer 1
     color 0 0 0 0.9608
     size 0.992 0.996
     halign center_ref
     valign center_ref
     hexactpos 0
     vexactpos 0
     hexactsize 0
     vexactsize 0
     style editor_selection
    }
    ImageWidgetClass header {
     ignorepointer 1
     color 0.6706 0.6706 0.6706 0.8784
     position 0 0.007
     size 0.99 0.03
     halign center_ref
     hexactpos 0
     vexactpos 0
     hexactsize 0
     vexactsize 0
     image0 "set:dayz_gui image:container_header"
     mode blend
     "src alpha" 1
     "no wrap" 1
     stretch 1
     {
      TextWidgetClass titletext {
       ignorepointer 1
       position 0 0
       size 1 1.207
       valign center_ref
       hexactpos 0
       vexactpos 0
       hexactsize 0
       vexactsize 0
       style Bold
       text "Text Editor"
       font "gui/fonts/sdf_MetronLight42"
       "exact text" 1
       "exact text size" 22
       "text halign" center
       "text valign" center
      }
      ButtonWidgetClass BtnClose {
       clipchildren 1
       color 0 0 0 1
       size 0.03 1
       halign right_ref
       hexactpos 0
       vexactpos 0
       hexactsize 0
       vexactsize 0
       style Editor
       {
        ImageWidgetClass ImageClose {
         ignorepointer 1
         size 0.97 1
         halign center_ref
         valign center_ref
         hexactpos 0
         vexactpos 0
         hexactsize 0
         vexactsize 0
         image0 "set:ccgui_enforce image:Icon28EmptySlot"
         mode additive
         "src alpha" 1
        }
       }
      }
     }
    }
    ScrollWidgetClass Scroller {
     ignorepointer 0
     size 0.99 0.9
     halign center_ref
     valign center_ref
     hexactpos 0
     vexactpos 0
     hexactsize 0
     vexactsize 0
     style blank
     "Ignore invisible" 0
     "Scrollbar V" 1
     {
      MultilineEditBoxWidgetClass TextInput {
       size 0.99 1
       halign center_ref
       hexactpos 0
       vexactpos 0
       hexactsize 0
       vexactsize 0
       style DayZBold
       font "gui/fonts/MetronBlack22"
       "exact text" 1
       text_proportion 1
       "size to text h" 1
       "size to text v" 1
       lines 5000
       "limit visible" 0
      }
     }
    }
    ButtonWidgetClass BtnApply {
     position 0 0.01
     size 0.25 0.035
     halign center_ref
     valign bottom_ref
     hexactpos 0
     vexactpos 0
     hexactsize 0
     vexactsize 0
     style Editor
     text "Apply"
    }
   }
  }
 }
}